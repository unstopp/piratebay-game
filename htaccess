# Set the default page
# ----------------------------------------------------
DirectoryIndex intro.php


# Set the 404 not found custom page
# ----------------------------------------------------
ErrorDocument 404 http://unstopp.com/game/404.html


# Rewrite rules for the URL
# ----------------------------------------------------
Options +FollowSymlinks
RewriteEngine on
RewriteRule ^(.*)\.html$ $1.php [NC]


